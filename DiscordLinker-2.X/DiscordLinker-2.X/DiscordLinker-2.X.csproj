  </ItemGroup>

  <ItemGroup>
    <Folder Include="Properties\IPCManage\" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\OxideFileIPC\OxideFileIPC.csproj" />
  </ItemGroup>

</Project>
